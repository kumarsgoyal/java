
import java.util.*;

public class count_word 
{

	public static void main(String[] args) 
	{
	    int i=0;
		  Scanner sc = new Scanner(System.in);
		  String s = sc.nextLine();
	  	Scanner s1 = new Scanner(s);
		  while(s1.hasNext())
		  {
			   i++;
			   s1.next();
		  }
		 sc.close();
		 System.out.println(i);
		 s1.close();
		

	}

}
